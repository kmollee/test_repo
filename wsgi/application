import cherrypy
class HelloWorld(object):
    def index(self):
        outstring = '''
        <html>
        <head>
            <title>this is example</title>
        </head>
        <body>
            <h1>網際網路應用</h1>
            <br />
            <a href="http://www.google.com">Go to google!</a>
        </body>        
        </html>
        '''
        return outstring
    index.exposed = True

cherrypy.quickstart(HelloWorld())
#application = cherrypy.Application(HelloWorld())